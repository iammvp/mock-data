{
    "extends": "airbnb-base",
    "rules": {
        "indent": ["error", 4, { "SwitchCase": 1 }],
        "no-console": 0,
        "eol-last": 0,
        "class-methods-use-this": 0,
        "array-callback-return": 0,
        "no-plusplus": 0,
        "no-mixed-operators": 0,
        "no-restricted-globals": 0,
        "no-case-declarations": 0,
        "consistent-return": 0,
        "import/no-dynamic-require": 0
    }
}