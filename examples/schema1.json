[
    {
        "key":"movieLists",
        "type":"array",
        "length":50,
        "schema":[
            {
                "key":"movieId",
                "type":"index",
                "start":10000
            },
            {
                "key":"movieName",
                "type":"word",
                "length":3
            },
            {
                "key":"movieType",
                "type":"enum",
                "value":["2d","3d","3d-max","i-max"]
            },
            {
                "key":"time",
                "type":"date",
                "range":["2017-05-01","2017-10-01"]
            },
            {
                "key":"score",
                "type":"number",
                "decimal":1,
                "range":[0,100]
            }
        ]
    }
]